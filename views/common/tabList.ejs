<div>
    <!--tablist start-->
    <ul class="nav nav-tabs" role="tablist">
        <li class="text-error"><h4>旅游出行&nbsp;&nbsp;</h4></li>
        <% tabList.forEach(function(item,index){ %>
            <% if (index==0) { %>
                <li class="active">
            <% } else { %>
                <li>
            <% } %>
            <a href="<%= '#'+item.id %>" onClick="return false;" data-toggle="tab"><%= item.name %></a></li>
        <% }) %>
    </ul>

    <!-- Tab panes start -->
    <div class="tab-content">
        <% tabList.forEach(function(tab,z){ %>
            <% if (z==0) { %>
                <div role="tabpanel" class="tab-pane active" id="<%= tab.id %>">
            <% } else { %>
                <div role="tabpanel" class="tab-pane" id="<%= tab.id %>">
            <% } %>
            <div class="list-group">
                <% tab.list.forEach(function(i,index){ %>
                    <%= i.Name.length>12?i.Name.substring(0,12)+'...':i.Name %>
                <% }) %>
            </div>
        </div>
        <% }) %>
    </div>
    <!-- end Tab panes -->
    <!--end tablist-->
</div>