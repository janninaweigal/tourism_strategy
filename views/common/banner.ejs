<div class="jumbotron bck margin-top-70">
    <div class="container">
        <div class="row-fluid">
            <img class="pull-left" src="images/logo.png" alt="logo" style="width:150px;height:130px;cursor:pointer;">
            <div class="pull-right" style="padding-top:30px">
                <div>
                    <span class="label label-danger">目的地：</span>
                </div>
                <form class="form-inline text-right" style="margin-top:3px">
                    <div class="form-group">
                        <input class="form-control" placeholder="请输入目的地" type="text" value="<%= searchName %>" maxlength="50" autocomplete="off"/>
                    </div>
                    <button type="button" class="btn btn-danger searchResult">搜索</button>
                </form>
            </div>
        </div>
    </div>
</div>